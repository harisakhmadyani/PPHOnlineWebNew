<div class="modal fade" id="modal-xl">
    <div class="modal-dialog modal-lg">
        <form id="my_form_insert">
            <div class="modal-content">
                <div class="modal-header bg-gradient-primary">
                    <h5 class="modal-title brand-text font-weight-light"><i class="far fa-file-alt mr-1"></i><b>Form</b> Group Access</h5>
                    <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                    </button>
                </div>
                <div class="modal-body">
                    <div class="col-md-12">
                        <div class="form-group row">
                            <label class="col-md-3 col-form-label">Group Access ID</label>
                            <div class="col-md-9">
                                <input type="text" class="form-control" id="groupAccessID" required>
                            </div>
                        </div>
                        <div class="form-group row">
                            <label class="col-md-3 col-form-label">Group Access Name</label>
                            <div class="col-md-9">
                                <input type="text" class="form-control" id="groupAccessName" required>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="modal-footer justify-content-between mr-1 ml-1">
                    <button type="button" class="btn btn-default" data-dismiss="modal"><i class="fas fa-times mr-1"></i>Close</button>
                    <button type="button" id="save" class="btn bg-gradient-primary" onclick="saveData()"><i class="fas fa-save mr-1"></i>Save</button>
                </div>
            </div>
        </form>
    </div> 
</div>


